{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "continue": true
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/server.js"
      }
    ],
    "static": {
      "public": "children"
    }
  }
  